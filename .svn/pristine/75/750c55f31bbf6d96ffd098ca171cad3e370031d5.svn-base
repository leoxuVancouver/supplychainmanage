            <label class="label_4">企业名称：</label>
            <input type="text" class="inputstyle" value="" />
            <label class="label_4">操作类型：</label>
            <select class="inputstyle">
            	<option>全部</option>
                <option>申请绑定</option>
                <option>拒绝绑定</option>
                <option>确认绑定</option>
