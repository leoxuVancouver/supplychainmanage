@charset "utf-8";
/* CSS Document */

/*页头*/
#login_head { width:100%; min-width:1160px; /*height:127px;*/ background-color: rgba(255,255,255,.8); /*border-bottom:3px #158DF3 solid;*/ position:fixed; top: 0; left: 0; z-index:9;}
#login_head .mid { width: 1160px; height:100%; overflow:hidden; margin: 0 auto;position: relative; z-index: 1000;}
.head_top { width:100%; height:50px; background:#0066b3; relative; z-index: 80; }
.head_top span{ line-height:50px; overflow:hidden;}
.head_top span a{ margin-right:20px; color:#83c6ff; line-height:50px; text-decoration:none;}
.head_top span a.kfrx { margin-right: 0;}
.head_top span a img { display:inline-block; margin-bottom:-10px; border: 0;}
.head_top span a:hover{  color:#fff; text-decoration:underline;}
.head_bottom { width:100%; height:77px; /*background:#fff;*/}
.head_bottom span{ line-height:77px; overflow:hidden;}
.head_bottom span img { display: inline-block; margin-bottom:-15px; border: 0 ;}
.head_bottom span a{; margin-right:10px; display:inline-block; height:42px; width:116px; line-height:42px; background:#3CF; color:#FFF; text-decoration:none; text-align:center; font-size:16px; cursor:pointer;}
.head_bottom span a:hover{ color:#FFF; background:#39F; text-decoration:none;}
.head_bottom span ul { overflow:hidden; display:inline-block; margin-bottom:-14px;}
.head_bottom span ul li { list-style:none; line-height:1.5em; font-size:14px; color:#666; cursor:pointer;}
.head_bottom span ul li:hover { color:#39f;}

/*滚屏*/
.login_main { overflow-y: hidden;}
.next { display:block; position:fixed; bottom: 100px; left:650px; cursor: pointer; z-index: 999}
.nav_wrap { position:fixed; top:300px; right: 20px; z-index: 9;}
.nav_wrap li { width: 14px; height: 14px; margin-bottom:15px;}
.nav_wrap li a { display:inline-block; width: 10px; height: 10px; border: 2px solid #0066b3; border-radius:100%;
	-webkit-transition:all 600ms ease;
	   -moz-transition:all 600ms ease;
	    -ms-transition:all 600ms ease;
		 -o-transition:all 600ms ease;
	        transition:all 600ms ease;}
.nav_wrap li a:hover { background:#0066b3;}
.nav_wrap li a.active { background:#0066b3;}
.login_main .part { width:100%; height:100%; margin: 0 auto; position: relative;}
.part > img { width: 100%; position: absolute; bottom: 0; left: 0;}
/*parts*/
.part1 { background-image: url(../images/login_banner/1.png); background-color:#fff; background-repeat: no-repeat; background-size:cover;}
.part2 { background-image: url(../images/login_banner/2.jpg); background-color:#fff; background-repeat: no-repeat; background-size:cover;}
.part3 { background-image: url(../images/login_banner/3.jpg); background-color:#fff; background-repeat: no-repeat; background-size:cover;}
.part4 { background-image: url(../images/login_banner/4.jpg); background-color:#fff; background-repeat: no-repeat; background-size:cover;}
.part5 { background-image: url(../images/login_banner/5.jpg); background-color:#fff; background-repeat: no-repeat; background-size:cover;}
.part6 { position: relative; }
.part6 .part6_top { width:100%; padding-top:77px; background:#EEE; overflow:hidden;}
.part6 .part6_bottom { position:absolute; bottom: 0; left: 0; width:100%; height:100px; overflow:hidden;}

/*新加样式 start*/
.clearfix {
	zoom: 1;
}
.clearfix:after {
	content: "";
	display: block;
	height: 0;
	clear: both;
	visibility: hidden;
}
.btn_bg{ background-color: #3399fe !important;}
.head_bottom .serviceHot_phone{ background: url(../images/serviceHot_phone.png) no-repeat; width: 165px; height: 42px; line-height: 42px; vertical-align: middle; display: inline-block;}
.pFixed{ position: fixed; right: 80px; cursor: pointer;}

.fn_hide{ display:block; !important;}

.banner_font{ position: relative; left: 100px; padding-bottom: 30px;}

.main-content{
	-webkit-transform: translate3d(0,0,0);
	position: relative;
	z-index: 0;
	width: 1200px;
	margin: 0 auto;
	height: 100%;
}
.banner_font2,.banner_font3,.banner_font4,.banner_font5{
	-webkit-transition: 1s .2s opacity;
	-o-transition: 1s .2s opacity;
	-moz-transition: 1s .2s opacity;
	-ms-transition: 1s .2s opacity;
	transition: 1s .2s opacity;
	visibility: visible;
	position: absolute;
	background-position: 0 0;
	background-repeat: no-repeat;
}
.banner_font2{
	top: 20%;
	left: 5%;
	/*padding-top: 13%;*/
	z-index: 11;
}
.banner_font3{
	top: 30%;
	left: 5%;
	/*padding-top: 25%;*/
	z-index: 11;
}
.banner_font4{
	top: 45%;
	left: 5%;
	/*padding-top: 25%;*/
	z-index: 11;
}
.banner_font5{
	top: 18%;
	left: 50%;
	/*padding-top: 8%;*/
	z-index: 11;
}


/*定位按钮 start*/
.laymid {
	position: absolute;
	top: 35%;
	left: 50%;
	width: 420px;
	height: 100px;
	margin-left: -195px;
	margin-top: -40px;
	z-index: 10;
}
.click-me a {
	display: block;
	text-decoration: none;
	float: left;
}
.button-login {
	letter-spacing: 5px;
	border-color: #00bbee;
}
.am-button {
	font-size: 16px;
	text-align: center;
	cursor: pointer;
	display: block;
	border-radius: 5px;
	outline: none;
	color: #fff;
	margin-right: 20px;
}
.button-login {
	width: 190px;
	line-height: 34px;
	height: 34px;
	background: #0065b3;
}
.button-register {
	width: 188px;
	line-height: 34px;
	height: 34px;
	border: 1px solid #FFF;
	letter-spacing: 5px;
}
.am-button:hover{ color: #FFF;}
/*定位按钮 end*/

.bgWhite{ background-color: #FFF;}
.bgWhite ul li img{ position: relative; top: 8px;}
.line_tip{ width: 383px; border-bottom: 1px solid #666; margin: 0 auto; margin-bottom: 20px;}
.line_tip_h2{ background-color: #EEE; width: 115px; position: relative; top: 7px; left: 35%; font-size: 16px; text-align: center; font-weight: bold;}
.mt50{ margin-top: 50px !important;}
.mb80{ margin-bottom: 80px !important;}
.mt30{ margin-top: 30px !important;}
.line_steps{ background: url(../images/login_steps_bg.png) no-repeat center; height: 34px;}
/*新加样式 end*/

/*解决方案*/
.finance-tag { border-bottom: 2px solid #eee;}
.mess-tit{ height:45px; background:#FFF url(../images/sol_tit_bg.png) no-repeat;}
.mess-tit h2{ font-size:20px; color:#333; float:left; padding-left:20px; line-height:30px;}
.finance-message .fangan_show{ width:1160px; height:250px; margin:0 auto; background:#FFF; position:relative; overflow: hidden;}
.finance-show{ width:1160px; height:auto; margin:0 auto; position:absolute; top:0; left:0; font-family:"微软雅黑";}
.finance-show>ul{ padding:10px 70px; overflow:hidden;}
.finance-show:last-child>ul{ padding:10px 200px;}
.finance-show:last-child>ul>li{ margin-right:200px;}
.finance-show>ul>li{ float:left; margin-right:80px; cursor:pointer;}
.finance-show>ul>li:last-child{ margin-right:0;}
.finance-show>ul>li>ul { width:280px; overflow:hidden;}
.finance-show>ul>li>ul>li>img{ padding-left:85px;}
.finance-show>ul>li>ul>li>h4{ font-size:20px; color:#1a1a1a; text-align:center;}
.finance-show>ul>li>ul>li:last-child{ width:160px; font-size:14px; color:#808080; margin:10px auto 0; text-align:left;}
.finance-message .show_bottom{ width:1160px; height:auto; margin: 0 auto; overflow:hidden;}
.finance-message .show_bottom .show_bottom_mid{ width:1160px; margin:20px auto; 
												background:rgb(238,238,238) url(../images/show_bottom_mid_bg.png) 50% 50% no-repeat;}
.finance-message .show_bottom .show_bottom_mid ul{ width:100%; height:97px; padding:5px; overflow:hidden;}
.finance-message .show_bottom .show_bottom_mid ul li{ float:left; padding-left:25px; width:550px; font-family:"微软雅黑";/* font-style:italic; */font-size:14px; color:#666;}
.finance-message .show_bottom .show_bottom_mid ul li span{ display:inline-block; height:40px; vertical-align:top; line-height:40px; color:#000; font-size:16px; font-style:normal; padding-left: 20px;}
.finance-message .show_bottom .show_bottom_mid ul li p { margin-left:70px; margin-top:-30px; padding-left: 20px;}

/*如何加入*/
