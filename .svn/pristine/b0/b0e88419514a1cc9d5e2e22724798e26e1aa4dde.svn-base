/*登录弹窗*/
.mask { display:none; position:absolute; top:0; left:0; z-index:100; opacity:0.5; filter:alpha(opacity=50); background:#000; width:100%; height:100%;}
.login_show{ display:none; width:356px; height:340px; background:rgba(0,0,0,.8); position:fixed; z-index:101; }
.login_show .login_top{ width:100%; height:54px;}
.login_show .login_top span{ display:inline-block; width:178px; height:54px; line-height:54px; text-align:center; color:#fff; font-family:"黑体"; font-size:20px; font-weight:bold; float:left; cursor:pointer; position:relative;}
.login_show .login_top span a{ font-size:20px; color:#428ac5; position:absolute; top:-16px; left:5px; }
.login_on{ color:#428ac5; background:#003057; opacity:60%;}
.login_main table{ margin:20px auto;}
.login_main table tbody tr td{ display:inline-block; margin-bottom:5px;}
.login_main table tbody tr td span{ display:block; font-size:12px; font-family:"黑体"; color:#ffba00; margin-bottom:10px;}
.login_main table tbody tr td b{ font-size:14px; font-family:"黑体"; color:#ffba00; font-weight:100; cursor:pointer; }
.login_main table tbody tr td strong{ font-size:14px; font-family:"黑体"; color:#fff; font-weight:100; padding-right:10px; cursor:pointer;}
.login_main table tbody tr td input{ width:270px; height:35px; text-indent:50px; border:0}
.login_main table tbody tr td input.login_id{ background: url(../images/login_id.png) 0 50% no-repeat #fff; text-indent:50px;}
.login_main table tbody tr td input.login_pw{ background: url(../images/login_pw.png) 0 50% no-repeat #FFF;}
.login_main table tbody tr td input.login_code{ width:104px; height:35px; text-indent:10px;}
.login_main table tbody tr td a{ display:block; width:270px; height:35px; text-align:center; line-height:35px; font-size:16px; font-family:"黑体"; color:#FFF; background:#1c6eb1;
background:-webkit-linear-gradient(top,#1b93f5,#1c6eb1);
background:-moz-linear-gradient(top,#1b93f5,#1c6eb1);
background:-ms-linear-gradient(top,#1b93f5,#1c6eb1);
background:-o-linear-gradient(top,#1b93f5,#1c6eb1);
background:linear-gradient(top,#1b93f5,#1c6eb1);}