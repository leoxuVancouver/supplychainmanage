#foot .foot_con ul{ float:right; margin-top:40px;}
#foot .foot_con ul li{ float:left; padding-right:10px;}
#foot .foot_con ul li a{ line-height:40px; color:#FFF; font-size:12px; text-decoration:none; cursor:pointer;}
#foot .foot_con ul li a:hover{ color:#FFF; text-decoration:none;}
/*foot end**********************************************************************************************************************/



