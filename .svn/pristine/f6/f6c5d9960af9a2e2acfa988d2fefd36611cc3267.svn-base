.nav_box > dl{ float:left; width:120px; border-right:1px #ddd dotted;}
.nav_box > dl > dt{ height: 30px; line-height: 30px; padding-left: 20px; border-bottom:1px #ddd dotted; font-size:15px; font-weight:600;}
.nav_box > dl > dd{ height: 30px; line-height: 30px; padding-left: 20px;}
.nav_box > dl > dd > a{font-size:13px; text-decoration:none; color:#000; cursor:pointer; }
.nav_box > dl > dd > a:hover{ text-decoration:none;  }


#sidebtn{ position:fixed; bottom: 0; right:20px; z-index:999;}
#sidebtn ul li{ height:60px; position:relative; margin-bottom: 10px;}
#sidebtn ul li a { display:block; height:60px;}
#sidebtn ul li span{ display:none; width:56px; height:52px; position:absolute; top:0; left:0; background:#FFF; color:#0066b3; font-size:14px; font-weight:bold; text-align:center; text-decoration:none;  padding-top:8px;}
#sidebtn ul li span:hover{ color:#0066b3; text-decoration:none;}
#sidebtn ul li span.sidebtn1{ padding-top:20px; height:40px;}
#sidebtn ul li:hover a span{ display:block;}
