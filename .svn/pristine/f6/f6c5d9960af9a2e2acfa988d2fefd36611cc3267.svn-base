@charset "utf-8";
/* CSS Document */
body {
	font-family:Tahoma, 'Microsoft YaHei';
	font-size:12px;
	color:#333;
	margin:0;
	background-color: #FFFFFF;
}
button, input, select, textarea { outline: none; resize: none; }
h1, h2, h3, h4, h5, h6, ul, ol, li, dl, dt, dd, p, textarea {
	margin: 0;
	padding: 0;
}
ul, ol, li { list-style: none; }
a { color: #4d4d4d; text-decoration: none;}
a.blue { color:#1a1a1a; text-decoration: none;}
a:hover {
	color: #2a83c7;
	text-decoration:none;
}
em,i { font-style:normal}
b { font-weight:normal}
.f_l {
	float:left;
}
.f_r { float:right;font-weight: bold;}
img { border:none}
.o_h { overflow:hidden}
.hide { display:none}
.show { display:block;}
input,select,textarea { font-family:Tahoma , 'Microsoft YaHei';}
.topright a{ color:#36C;}
.topright a:hover{ color:#F60;}

.info{ width:100%;  border:1px solid #f1f3f7;}
.info table{ padding:10px;}
.t_a_r{ text-align:right;}
.t_a_c{ text-align:center;}
.t_a_l{ text-align:left;}
.dis_i{ display:inline;}
.dis_ib{ display:inline-block;}
#info span{ color:#F00;}
#info th{ height:40px; text-align:right;}
#info p{ display:table-cell; vertical-align:top;}
.red{ color:red;}
.green{ color:#79cc6e;}
.blue{ color:#0066b3;}
.clear_both { clear:both; width:100%; height:0; border:0; padding:0; margin:0;}

/*newhead begin*************************************************************************************************************************/
#header_html{ width:100%; min-width:1160px; height:127px; background:#FFF; 
border-bottom:3px #158DF3 solid;}
#user_info{
	width:100%;
	height:50px;
	background-color: #f2f2f2;
	font-size: 14px;
}
#user_info .user_info_mid{ width:1160px; height:50px; margin:0 auto;}
