#user_info .user_info_mid a img { display:inline-block; margin-bottom:-10px; border: 0;}
#user_info .user_info_mid a:hover{  color:#ff0000; text-decoration:underline;}

#nav{ width:1160px; height:77px; margin:0 auto; background:url(../images/logo.jpg) 0 50% no-repeat;
 position:relative; z-index:99;}
#nav .nav_list{ position:absolute; bottom:25px; right:0; }
#nav .nav_list li{ float:left;width: auto;  position: relative;}
#nav .nav_list li>a{ padding:5px 25px; font-size:16px; color:#000; text-decoration:none;}
#nav .nav_list li:hover > a{ color:#fff; text-decoration:none; background:#1693f0; }

.nav_on{ color:#1693f0;}

#nav .nav_box{ position:absolute; top:26px; padding:20px 0px; display:none; 
	background:#fff; border:1px #b8d7ed solid; border-top:#1693f0 2px solid;}
#nav .nav_list li:hover .nav_box{ display: block;}

#nav .nav_list li.procure .nav_box{ width: 242px; left:-80px; }
#nav .nav_list li.sale .nav_box{ width: 363px; left:-140px;}
#nav .nav_list li.caiwu .nav_box{ width: 242px; left:-80px;}
#nav .nav_list li.fahuo .nav_box{ width: 242px; left:-80px;}
#nav .nav_list li.datas .nav_box{ width: 242px; left:-70px;}
#nav .nav_list li.system .nav_box{ width: 484px;left:-186px;}
#nav .nav_list li.check .nav_box{ width: 242px; left:-68px;}
